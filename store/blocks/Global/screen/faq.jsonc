{
  "store.custom#landing-faq": {
    "title": "landing faq",
    "blocks": [
      "flex-layout.row#faq-box--one",
      "flex-layout.row#faq-container"
    ]
  },

  "flex-layout.row#faq-container": {
    "children": ["tab-layout#faq"],
    "props": {
      "blockClass": "faq-container"
    }
  },

  "flex-layout.row#faq-box--one": {
    "children": ["rich-text#preguntas"],
    "props": {
      "blockClass": "preguntas"
    }
  },

  "rich-text#preguntas": {
    "props": {
      "text": "Preguntas Frecuentes",
      "blockClass": "preguntas"
    }
  },

  "tab-layout#faq": {
    "children":[
      "tab-list#faq",
      "tab-content#faq"
    ],
    "props":{
      "blockClass": "faq",
      "defaultActiveTabId": "faq-pagos"
    }
  },

  "tab-list#faq": {
    "children": [
      "tab-list.item#tab1",
      "tab-list.item#tab2",
      "tab-list.item#tab3",
      //"tab-list.item#tab4",
      "tab-list.item#tab5"
    ],
    "props": {
     "blockClass": "faq-tab--list"
    }
  },

  "tab-list.item#tab1": {
    "props": {
      "tabId": "faq-pagos",
      "label": "Pagos",
      "defaultActiveTab": true,
      "blockClass": "tab-list"
    }
  },

  "tab-list.item#tab2": {
   "props": {
     "tabId": "faq-devoluciones",
     "label": "Devoluciones",
     "blockClass": "tab-list"
    }
  },

  "tab-list.item#tab3": {
   "props": {
     "tabId": "faq-envios",
     "label": "Envíos",
     "blockClass": "tab-list"
    }
  },

  "tab-list.item#tab4": {
    "props": {
      "tabId": "faq-tiendas",
      "label": "Tiendas físicas",
      "blockClass": "tab-list"
     }
   },

   "tab-list.item#tab5": {
    "props": {
      "tabId": "faq-empresa",
      "label": "Empresa",
      "blockClass": "tab-list"
     }
   },

   "tab-content#faq": {
    "children": [
      "tab-content.item#tab1",
      "tab-content.item#tab2",
      "tab-content.item#tab3",
      "tab-content.item#tab4",
      "tab-content.item#tab5"
    ],
    "props":{
      "blockClass": "faq-content"
    }
  },

  "tab-content.item#tab1": {
    "children": ["disclosure-layout-group#content-faq-1"],
    "props": {
      "tabId": "faq-pagos",
      "blockClass": "tab1"
    }
  },

  "tab-content.item#tab2": {
    "children": ["disclosure-layout-group#content-faq-2"],
    "props": {
      "tabId": "faq-devoluciones",
      "blockClass": "tab2"
    }
  },

  "tab-content.item#tab3": {
    "children": ["disclosure-layout-group#content-faq-3"],
    "props": {
      "tabId": "faq-envios",
      "blockClass": "tab3"
    }
  },

  "tab-content.item#tab4": {
    "children": ["disclosure-layout-group#content-faq-4"],
    "props": {
      "tabId": "faq-tiendas",
      "blockClass": "tab4"
    }
  },

  "tab-content.item#tab5": {
    "children": ["disclosure-layout-group#content-faq-5"],
    "props": {
      "tabId": "faq-empresa",
      "blockClass": "tab5"
    }
  },

  "disclosure-layout-group#content-faq-1": {
    "children": [
      "disclosure-layout#one",
      "disclosure-layout#two",
      "disclosure-layout#three",
      "disclosure-layout#four" ,
      "disclosure-layout#five"
    ],
    "props":{
      "blockClass": "content-faq"
    }
  },

  "disclosure-layout-group#content-faq-2": {
    "children": [
      "disclosure-layout#one",
      "disclosure-layout#two",
      "disclosure-layout#three"/* ,
      "disclosure-layout#four" ,
      "disclosure-layout#five" */
    ],
    "props":{
      "blockClass": "content-faq"
    }
  },

  "disclosure-layout-group#content-faq-3": {
    "children": [
      "disclosure-layout#one",
      "disclosure-layout#two",
      "disclosure-layout#three"/* ,
      "disclosure-layout#four" ,
      "disclosure-layout#five" */
    ],
    "props":{
      "blockClass": "content-faq"
    }
  },

  "disclosure-layout-group#content-faq-4": {
    "children": [
      "disclosure-layout#one",
      "disclosure-layout#two",
      "disclosure-layout#three",
      "disclosure-layout#four" ,
      "disclosure-layout#five"
    ],
    "props":{
      "blockClass": "content-faq"
    }
  },

  "disclosure-layout-group#content-faq-5": {
    "children": [
      "disclosure-layout#one"/* ,
      "disclosure-layout#two",
      "disclosure-layout#three",
      "disclosure-layout#four" ,
      "disclosure-layout#five" */
    ],
    "props":{
      "blockClass": "content-faq"
    }
  },

  /* primero */

  "disclosure-layout#one": {
    "children": ["disclosure-trigger#one", "disclosure-content#one"],
    "props": {
      "blockClass": "disclosure-layout-one"
    }
  },

  "disclosure-trigger#one": {
    "children": ["rich-text#one", "disclosure-state-indicator"],
    "props":{
      "as": "div",
      "blockClass": "trigger-one"
    }
  },
  "disclosure-content#one": {
    "children": ["rich-text#text2"],
    "props": {
      "blockClass": "content-one"
    }
  },

  "rich-text#one": {
    "props": {
      "text": "¿Como puedo pagar?",
      "blockClass": "one"
    }
  },
  "disclosure-state-indicator": {
    "props": {
      "Show": "icon-caret#down",
      "Hide": "icon-caret#up"
    }
  },
  "icon-caret#down": {
    "props": {
      "orientation": "down"
    }
  },
  "icon-caret#up": {
    "props": {
      "orientation": "up"
    }
  },

/* segundo */
  "disclosure-layout#two": {
    "children": ["disclosure-trigger#two", "disclosure-content#two"],
    "props": {
      "blockClass": "disclosure-layout-one"
    }
  },

  "disclosure-trigger#two": {
    "children": ["rich-text#two", "disclosure-state-indicator"],
    "props":{
      "as": "div",
      "blockClass": "trigger-one"
    }
  },
  "disclosure-content#two": {
    "children": ["rich-text#texto"],
    "props": {
      "blockClass": "content-two"
    }
  },

  "rich-text#two": {
    "props": {
      "text": "¿Qué debo hacer para comprar con Pago Contra Entrega?",
      "blockClass": "one"
    }
  },

/* tercero */
