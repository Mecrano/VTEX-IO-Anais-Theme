    "title": "brand 1",
    "children": [
      "image#brand-1-image-1",
      "image#brand-1-image-2",
      "rich-text#brand-see-more"
    ],
    "props": {
      "blockClass": "home--brands--container-brand-1",
      "width": "33%"
    }
  },
  "image#brand-1-image-1": {
    "props": {
      "src": "assets/images/Home/adidas-baner.jpg",
      "blockClass": "home--brands--banner"
    }
  },
  "image#brand-1-image-2": {
    "props": {
      "src": "assets/images/Home/brand-adidas.jpg",
      "blockClass": "home--brands--images"
    }
