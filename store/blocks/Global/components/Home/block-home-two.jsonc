  "rich-text#benefit-3": {
    "props": {
      "text": "Tus compras siempre seguras",
      "textAlignment": "LEFT",
      "textPosition": "CENTER",
      "blockClass": ["home--benefits-benefit", "home--benefits-benefit3"]
    }
  },

  /* 4 benefit */
  "flex-layout.row#benefit-4": {
    "children": [
      "icon#benefit-4",
      "rich-text#benefit-4"
    ],
    "props": {
      "blockClass": ["home--icon-benefit--container", "home--icon-benefit--container-4"],
      "preserveLayoutOnMobile": true
    }
